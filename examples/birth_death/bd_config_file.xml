		<experiments>0</experiments>
		<datafile>./bd_data.txt</datafile>
		<timefile>./bd_data_times.txt</timefile>		
	</dataset>
</data>
<Simulation>
	<experiments>0</experiments>
	<initialtime>0</initialtime>
	<finaltime>100</finaltime>
