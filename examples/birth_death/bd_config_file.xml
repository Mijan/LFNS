<model>
	<type>DET</type>
	<model>./bd_model.txt</model>
	<initialvalue>./bd_initial.txt</initialvalue>
	<measurement>./bd_measurement.txt</measurement>
</model>
<parameters>
	<scales>
        <parameters scale="log">k gamma</parameters>
    </scales>
	<bounds>
