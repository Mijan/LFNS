			<parameters>k gamma</parameters>
			<lowerbounds>0.6310</lowerbounds>
			<upperbounds>2.5119</upperbounds>
		</bound>
    </bounds>
    <fixedparams>
        <parameters>mu</parameters>
        <values>10</values>
    </fixedparams>
</parameters>
<data>
